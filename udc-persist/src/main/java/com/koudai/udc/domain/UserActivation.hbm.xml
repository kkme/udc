<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.koudai.udc.domain">
	<class name="UserActivation" table="user_activation">
		<id column="id" name="id" type="long">
			<generator class="identity" />
		</id>
		<property name="userId" column="userID" not-null="true" type="string" />
		<property name="yellowDiamond" column="yellowDiamond" type="boolean" />
		<property name="iosPay" column="iosPay" type="boolean" />
		<property name="alreadyTest" column="alreadyTest" type="boolean" />
	</class>
</hibernate-mapping>

