<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean name="GetEditorQuantitativeData.spring.action" class="com.koudai.udc.statis.action.GetEditorQuantitativeDataAction" scope="prototype">
		<property name="productRecommendDAO">
			<ref bean="productRecommendDAO" />
		</property>
		<property name="productStatisDAO">
			<ref bean="productStatisDAO" />
		</property>
	</bean>

	<bean name="GetProductQuantitativeData.spring.action" class="com.koudai.udc.statis.action.GetProductQuantitativeDataAction" scope="prototype">
		<property name="productRecommendDAO">
			<ref bean="productRecommendDAO" />
		</property>
		<property name="productStatisDAO">
			<ref bean="productStatisDAO" />
		</property>
	</bean>

	<bean name="GetProductstatisData.spring.action" class="com.koudai.udc.statis.action.GetProductstatisDataAction" scope="prototype">
		<property name="productRecommendDAO">
			<ref bean="productRecommendDAO" />
		</property>
		<property name="productStatisDAO">
			<ref bean="productStatisDAO" />
		</property>
	</bean>

	<bean name="GetProductTypeProportion.spring.action" class="com.koudai.udc.statis.action.GetProductTypeProportionAction" scope="prototype">
		<property name="productRecommendDAO">
			<ref bean="productRecommendDAO" />
		</property>
		<property name="productStatisPeriodDAO">
			<ref bean="productStatisPeriodDAO" />
		</property>
		<property name="productTypeDAO">
			<ref bean="productTypeDAO" />
		</property>
	</bean>

</beans>